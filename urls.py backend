from django.contrib import admin
from django.urls import path, include
from django.conf import settings
from django.conf.urls.static import static
from store import views  # Import your views module

urlpatterns = [
    path('', views.index, name='index'),  # URL pattern for the root URL
    path('mens', views.mens, name='mens'),  # URL pattern for the 'mens' page
    path('women', views.women, name='women'),  # URL pattern for the 'women' page
    path('about', views.about, name='about'),  # URL pattern for the 'about' page
    path('registeration', views.registration, name='registration'),  # URL pattern for the 'registration' page
    path('admin', admin.site.urls),
    path('accounts', include('django.contrib.auth.urls')),
    # Add your app's URLs here
] + static(settings.MEDIA_URL, document_root=settings.MEDIA_ROOT)
